{
  "manifest_version": 3,
  "name": "ScriptKiddie",
  "version": "0.1",
  "description": "Script kiddie",
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab",
    "contextMenus",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "obj/service_worker.js"
  },
  "icons": {
    "16": "img/favicon-16x16.png",
    "32": "img/favicon-32x32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://bydeluxe.atlassian.net/*"
      ],
      "js": [
        "obj/jira-content-script.js"
      ]
    }
  ]
}