{
  "name": "script-kiddie",
  "version": "0.1",
  "description": "Script kiddie",
  "permissions": [
    "declarativeContent", "storage", "activeTab", "contextMenus",
    "tabs", "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "icons": {
    "16": "img/favicon-16x16.png",
    "32": "img/favicon-32x32.png"
  },
  "manifest_version": 3
}